{
    "version": 2,
    "builds": [
        {
            "src": "backend/server.js",
            "use": "@vercel/node"
        },
        {
            "src": "backend/models/**/*.js",
            "use": "@vercel/node"
        },
        {
            "src": "backend/routes/**/*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "backend/server.js"
        }
    ]
}
