{
  "version": 2,
  "rewrites": [
    {
      "source": "/assets/(.)",
      "destination": "/assets/$1"
    },
    {
      "source": "/(.)",
      "destination": "/"
    }
  ],
  "builds": [
    {
      "src": "/package.json",
      "use": "@vercel/static-build"
    },
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}