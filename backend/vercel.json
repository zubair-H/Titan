{
    "version": 2,
    "builds": [
      {
        "src": "front-end/package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "dist" }
      },
      {
        "src": "back-end/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "back-end/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "front-end/dist/index.html"
      }
    ]
  }